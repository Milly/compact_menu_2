<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">

		<em:id>{57068FBE-1506-42ee-AB02-BD183E7999E4}</em:id>
		<em:name>Compact Menu 2</em:name>
		<em:version>1.7.2-2</em:version>
		<em:description>Duplicates the menubar on the toolbar as a menu of menus.</em:description>
		<em:creator>Milly</em:creator>
		<em:homepageURL>https://addons.mozilla.org/firefox/108029/author/</em:homepageURL>
		<em:optionsURL>chrome://compact/content/compactPrefDialog.xul</em:optionsURL>
		<em:iconURL>chrome://compact/skin/compact.png</em:iconURL>                
		<em:type>2</em:type>

		<!-- require Firefox 1.0 -->
		<em:file>
			<Description about="urn:mozilla:extension:file:compact.jar">
				<em:package>content/compact/</em:package>
				<em:skin>skin/classic/compact/</em:skin>
				<em:locale>locale/en-US/compact/</em:locale>
				<em:locale>locale/de-DE/compact/</em:locale>
				<em:locale>locale/fr-FR/compact/</em:locale>
				<em:locale>locale/it-IT/compact/</em:locale>
				<em:locale>locale/ja-JP/compact/</em:locale>
				<em:locale>locale/nl-NL/compact/</em:locale>
				<em:locale>locale/sv-SE/compact/</em:locale>
			</Description>
		</em:file>

		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>1.0</em:minVersion>
				<em:maxVersion>2.0.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>

	</Description>

</RDF>
